	<bean id="uniteArticleDomaine"
		class="com.gpro.consulting.tiers.commun.domaine.impl.UniteArticleDomaineImpl">
		<property name="uniteArticlePersistance" ref="uniteArticlePersistance" />
	</bean>

	<!-- bean produit -->

	<bean id="produitDomaine"
		class="com.gpro.consulting.tiers.commun.domaine.impl.ProduitDomaineImpl">
