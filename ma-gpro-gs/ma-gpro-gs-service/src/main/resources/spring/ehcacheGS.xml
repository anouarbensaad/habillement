<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="classpath:/spring/ehcache.xsd"
	updateCheck="false">

	<!-- Default Cache Configuration, with name 'default' -->
	<defaultCache maxElementsInMemory="300" eternal="false"
		overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />

	<cache name="ListeMagasinCache" maxElementsInMemory="1000"
		eternal="false" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />

	<cache name="ListeEmplacementCache" maxElementsInMemory="1000"
		eternal="false" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />

	<cache name="ListeRaisonCache" maxElementsInMemory="1000"
		eternal="false" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />

   <cache name="ListeFournisseurCache" maxElementsInMemory="1000"  
 	eternal="false" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" /> 
 	
 	<cache name="ListeClientCache" maxElementsInMemory="1000"  
 	eternal="false" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />
 	
 	<cache name="rechercherBonMouvementParId" maxElementsInMemory="1000" eternal="false"
		overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />  
		
	<cache name="rechercherClientParId" maxElementsInMemory="1000" eternal="false"
		overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />  
		
</ehcache>